<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>RxFixsServlet</servlet-name>
    <servlet-class>ncku.pjay.malteseann.server.RxFixsServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>RxFixsServlet</servlet-name>
    <url-pattern>/rxfix</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>FixServiceImpl</servlet-name>
    <servlet-class>ncku.pjay.malteseann.server.FixServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>FixServiceImpl</servlet-name>
    <url-pattern>/malteseann/fix</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>MalteseAnn.html</welcome-file>
  </welcome-file-list>

</web-app>
